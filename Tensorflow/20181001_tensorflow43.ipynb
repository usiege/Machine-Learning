   "source": [
    "import tensorflow as tf\n",
    "\n",
    "w = tf.Variable(tf.zero([2, 1]), name='weights')\n",
    "b = tf.Variable(0., name='bias')\n",
    "\n",
    "def inference(X):\n",
    "    return tf.matmul(X, w) + b\n",
    "\n"
   ]
  }
